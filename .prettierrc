{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "printWidth": 100,
  "endOfLine": "auto",
  "plugins": [
    "@trivago/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ],
  "importOrder": [
    "^@/app/globals.css$",
    "^[./].*\\.css$",
    "^react$",
    "^react-dom$",
    "^next",
    "^@mui/material-nextjs",
    "^@mui/material$",
    "^@mui/material/(.*)$",
    "^@mui/icons-material",
    "^@mui/x-(.*)$",
    "^@emotion/(.*)$",
    "^@react-three/(.*)$",
    "^three$",
    "^mapbox-gl$",
    "^react-map-gl$",
    "^@neondatabase/(.*)$",
    "^dayjs$",
    "<THIRD_PARTY_MODULES>",
    "^@/theme$",
    "^@/components/(.*)$",
    "^@/(.*)$",
    "^\\.\\./(.*)$",
    "^\\./(?!.*\\.(css|scss))(.*)$",
    "^\\./.*\\.(css|scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseInsensitive": true
}
